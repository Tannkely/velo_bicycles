{
  "name": "bicycles",
  "version": "1.0.0",
  "description": "Стартовый шаблон сборки Лиги А",
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "1.0.0",
    "autoprefixer": "10.4.0",
    "browser-sync": "2.27.7",
    "del": "6.0.0",
    "eslint-config-htmlacademy": "^5.0.0",
    "gulp": "4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-dart-sass": "^1.0.2",
    "gulp-dart-scss": "^1.1.0",
    "gulp-htmlmin": "5.0.1",
    "gulp-less": "5.0.0",
    "gulp-libsquoosh": "^1.0.12",
    "gulp-newer": "^1.4.0",
    "gulp-plumber": "1.2.1",
    "gulp-postcss": "9.0.1",
    "gulp-posthtml": "^3.0.5",
    "gulp-rename": "2.0.0",
    "gulp-svgmin": "^4.1.0",
    "gulp-svgstore": "8.0.0",
    "gulp-terser": "2.1.0",
    "gulp-webp": "4.0.1",
    "node-sass": "^6.0.1",
    "postcss": "8.3.11",
    "postcss-csso": "5.0.1",
    "postcss-scss": "^4.0.2",
    "posthtml-include": "^1.7.2",
    "stylelint": "14.1.0",
    "stylelint-config-htmlacademy": "0.1.11",
    "stylelint-order-htmlacademy": "^2.0.3"
  },
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint --fix \"source/scss/**/*.scss\" --custom-syntax postcss-scss",
    "test": "npm run editorconfig && npm run stylelint & eslint source/js/modules",
    "build": "gulp build",
    "start": "gulp"
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "not ie <= 11",
    "not op_mini all",
    "not < 0.25%"
  ],
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "source/*.html",
    "source/js/**/*.js",
    "source/img/**/*.svg",
    "source/scss/**/*.scss"
  ],
  "type": "module"
}
